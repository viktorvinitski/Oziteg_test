lex-start;\n  height: 70px;\n  box-sizing: border-box;\n  padding: 0 50px;\n  box-shadow: 0 6px 20px -4px rgba(0, 0, 0, .2);\n  position: relative;\n  z-index: 1;\n}\n\n.navbar__item1,\n.navbar__item2,\n.navbar__item3,\n.navbar__item4,\n.navbar__item5,\n.navbar__item6{\n  display: flex;\n  align-items: center;\n  box-sizing: border-box;\n  padding: 0 20px;\n  position: relative;\n  height: 100%;\n  &:hover{\n    background-color: #eaeaea;\n    border-bottom: 5px solid orange;\n    cursor: pointer;\n  }\n}\n.dropdown__menu{\n  display: none;\n  width: 100%;\n  position: absolute;\n  padding: 0 35px;\n  box-sizing: border-box;\n}\n.dropdown__menu-wrapper{\n  width: 100%;\n  background-color: #fff;\n  height: 100%;\n  box-sizing: border-box;\n  padding: 20px;\n}\n\n.drop__items1,\n.drop__items2,\n.drop__items3,\n.drop__items4,\n.drop__items5,\n.drop__items6{\n  display: none;\n  justify-content: space-between;\n  & a:first-child{\n    font-size: 18px;\n    margin-bottom: 10px;\n  }\n  & a{\n    margin-top: 5px;\n    color: #848484;\n    display: block;\n    cursor: pointer;\n    text-decoration: none;\n  }\n  & a:hover{\n    color: #232323;\n  }\n}\n\n.content{\n  padding: 20px 35px;\n}\n\n.content__item{\n  margin-top: 20px;\n  &:first-child{\n    margin: 0;\n  }\n}\n\n.navbar__burger{\n  align-self: center;\n  margin-left: auto;\n  width: 25px;\n  height: 25px;\n  background: url(\"../img/burger-img.png\") no-repeat center;\n  background-size: cover;\n  display: none;\n  &:hover{\n    opacity: .5;\n  }\n}\n.navbar__close{\n  align-self: center;\n  margin-left: auto;\n  width: 25px;\n  height: 25px;\n  background: url(\"../img/burger_close.svg\") no-repeat center;\n  background-size: cover;\n  display: none;\n  &:hover{\n    opacity: .5;\n  }\n}\n.mobile__navbar{\n  display: none;\n  width: 100%;\n  position: absolute;\n  z-index: 5;\n  background-color: #ffffff;\n}\n@media screen and (max-width: 1024px){\n\n  .navbar__item1,\n  .navbar__item2,\n  .navbar__item3,\n  .navbar__item4,\n  .navbar__item5,\n  .navbar__item6{\n    display: none;\n  }\n  .navbar__burger{\n    display: block;\n  }\n  .navbar{\n    padding: 0 20px;\n  }\n}\n\n\n// ==========accordeon===============\n\n.accordion,\n.subAcc{\n  display: flex;\n  align-items: center;\n  background-color: #ffffff;\n  color: #444;\n  cursor: pointer;\n  padding: 18px;\n  width: 100%;\n  text-align: left;\n  border: none;\n  outline: none;\n  transition: .4s;\n  border: 1px solid #efefef;\n  &:hover{\n    background-color: #ccc;\n  }\n  &::after{\n    content: '';\n    background: url(\"../img/down_arrow.svg\");\n    width: 20px;\n    height: 20px;\n    margin-left: 5px;\n  }\n}\n.subAcc{\n  padding-left: 35px;\n}\n\n.mobile__navbar span{\n  width: 2px;\n  height: 20px;\n  margin-left: auto;\n  background-color: #c7c7c7;\n}\n\n.active{\n  background-color: #c7c7c7;\n  &::after{\n    transform: rotate(180deg);\n    transition: .8s;\n  }\n}\n.mobile__navbar p{\n  padding: 18px 45px 18px;\n\n}\n.panel, .subPanel {\n  background-color: #f3f3f3;\n  display: none;\n  overflow: hidden;\n}\n\n\n\n\n"},"lineCount":null}},"error":null,"hash":"e54b35833797d7d6875b378db423e068","cacheData":{"env":{}}}